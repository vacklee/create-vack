{
  "name": "<%= packageName %>",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    <%_ if (enable(/^lint/)) { -%>
    "prepare": "husky install",
    <%_ } -%>
    <%_ if (enable('lint-commit')) { -%>
    "commit-msg": "commitlint --config .commitlintrc.js -e -V",
    <%_ } -%>
    <%_ if (enable('eslint')) { -%>
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore --max-warnings 0",
    <%_ } -%>
    <%_ if (enable('lint-staged')) { -%>
    "lint-staged": "lint-staged",
    <%_ } -%>
    "dev": "vite --mode dev",
    "build:dev": "vite build --mode dev",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode prod",
    "preview": "vite preview"
  },
  <%_ if (enable('lint-staged')) { -%>
  "lint-staged": {
    "**/*.{js,jsx,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  <%_ } -%>
  <%_ if (enable('lint-commit')) { -%>
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  },
  <%_ } -%>
  "dependencies": {
    "axios": "^1.3.3",
    "lodash": "^4.17.21",
    "pinia": "^2.0.30",
    "vue": "^3.2.45",
    <%_ if (enable('i18n')) { -%>
    "vue-i18n": "^9.2.2",
    <%_ } -%>
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    <%_ if (enable('lint-commit')) { -%>
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    <%_ } -%>
    <%_ if (enable('mock')) { -%>
    "@types/mockjs": "^1.0.7",
    <%_ } -%>
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    <%_ if (enable('mock')) { -%>
    "body-parser": "^1.20.1",
    "chokidar": "^3.5.3",
    <%_ } -%>
    <%_ if (enable('lint-commit')) { -%>
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.7",
    "cz-conventional-changelog-zh": "^0.0.2",
    <%_ } -%>
    <%_ if (enable('eslint')) { -%>
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    <%_ } -%>
    <%_ if (enable(/^lint/)) { -%>
    "husky": "^8.0.3",
    <%_ } -%>
    <%_ if (enable('lint-staged')) { -%>
    "lint-staged": "^13.2.3",
    <%_ } -%>
    <%_ if (enable('mock')) { -%>
    "mockjs": "^1.1.0",
    <%_ } -%>
    "sass": "^1.57.0",
    "unplugin-vue-define-options": "^1.1.1",
    "vite": "^4.1.2"
  }
}
