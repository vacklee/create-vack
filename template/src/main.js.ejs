import { createApp } from 'vue';
import { createPinia } from 'pinia';
import App from './App.vue';
<%_ if (enable('i18n')) { -%>
import i18n, { initLocale } from './locales';
<%_ } -%>
import router from './router';
import components from './components';
import './styles';

const app = createApp(App)
<%_ if (enable('i18n')) { -%>
  .use(i18n)
<%_ } -%>
  .use(createPinia())
  .use(router)
  .use(components);

(async () => {
  <%_ if (enable('i18n')) { -%>
  initLocale();
  <%_ } -%>
  app.mount('#app');
})();
